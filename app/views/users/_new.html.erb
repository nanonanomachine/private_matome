<% users.each do |user| %>
	<li>
		<div class ="row">
			<div class="small-4 medium-4 large-4 columns">
				<%= user_avatar_for user, 50 %>
			</div>
			<div class="small-8 medium-8 large-8 columns">
				<h6 class="username"><%= user.name %></h6>
					<%= link_to 'Add user to this group', group_user_path(group, user),
					method: :post
					%>
			</div>
	</li>
<% end %>